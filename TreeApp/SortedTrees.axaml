<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="TreeApp.SortedTrees"
        Loaded="Control_OnLoaded"
        Title="TreeApp">
        <Grid RowDefinitions="Auto,*">

                <StackPanel Grid.Row="0" Orientation="Horizontal" Background="Black" Margin="10">
                        <Button Content="Back to Menu" Click="BackButton_OnClick" Margin="0,0,10,0"/>
                </StackPanel>
        
                <Canvas Grid.Row="1" Name="MainCanvas" Background="Cornsilk">
                
                </Canvas>
        </Grid>
        
</Window>
